{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Default",
	"compilerOptions": {
		"composite": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"noUnusedLocals": false,
		"isolatedModules": true,
		"noUnusedParameters": false,
		"strict": true
	}
}
